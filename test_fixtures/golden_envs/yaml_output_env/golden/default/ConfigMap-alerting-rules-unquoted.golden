apiVersion: v1
data:
  alerts.rules: |-
    groups:
    - name: "prometheus-extra"
      rules:
      - alert: "PromScrapeFailed"
        annotations:
          message: "Prometheus failed to scrape a target {{ $labels.job }} / {{ $labels.instance }}"
          test: 0.00002
          test10: "hello + 0.90000000000000002"
          test2: "2.0000000000000002e-05"
          test3: "0.000020"
          test4: 0.00001
          test5: "1.0000000000000001e-05"
          test6: "0.000010"
          test7: "0.90000000000000002"
          test8: "0.900000"
          test9: "0.90000000000000002"
        expr: "up != 1"
        for: "15m"
        labels:
          severity: "warning"
      - alert: "PromScrapeFlapping"
        annotations:
          message: "Prometheus target flapping {{ $labels.job }} / {{ $labels.instance }}"
          runbook_url: "https://uctufr.xhf/omiuwrh/vkyybhbzkm_icgsw/kgup/muoeie/hxrr/ixsnsm/lheqpcyb.de#UfzvntcMmtoQiravxdvryiy"
        expr: "avg_over_time(up[5m]) < 1"
        for: "15m"
        labels:
          severity: "warning"
kind: ConfigMap
metadata:
  name: alerting-rules-unquoted
  namespace: default
