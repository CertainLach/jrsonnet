  alerts.rules.manifestYamlFromJson: |
    groups:
        - name: prometheus-extra
          rules:
            - alert: PromScrapeFailed
              annotations:
                message: Prometheus failed to scrape a target {{ $labels.job }} / {{ $labels.instance }}
                test: 2e-05
                test2: "2.0000000000000002e-05"
                test3: "0.000020"
                test4: 1e-05
                test5: "1.0000000000000001e-05"
                test6: "0.000010"
                test7: "0.90000000000000002"
                test8: "0.900000"
                test9: "0.90000000000000002"
                test10: hello + 0.90000000000000002
              expr: up != 1
              for: 15m
              labels:
                severity: warning
            - alert: PromScrapeFlapping
              annotations:
                message: Prometheus target flapping {{ $labels.job }} / {{ $labels.instance }}
                runbook_url: https://uctufr.xhf/omiuwrh/vkyybhbzkm_icgsw/kgup/muoeie/hxrr/ixsnsm/lheqpcyb.de#UfzvntcMmtoQiravxdvryiy
              expr: avg_over_time(up[5m]) < 1
              for: 15m
              labels:
                severity: warning
  bytes_threshold: 7.5e+06
  description: Automatic connection preference. Set to true for 'ACCEPT_AUTOMATIC'
    or false for 'ACCEPT_MANUAL'
  expr: max by (namespace, provider, collector) (cloudcost_exporter_collector_last_scrape_duration_seconds)
    > 50
  message: There are running tests that don't have any metric rows inserted for 10
