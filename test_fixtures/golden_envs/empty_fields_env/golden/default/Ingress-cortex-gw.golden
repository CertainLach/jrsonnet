apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  annotations:
    kubernetes.io/ingress.class: nginx
    nginx.ingress.kubernetes.io/ssl-redirect: "true"
  name: cortex-gw
  namespace: default
spec:
  rules:
  - host: cortex.example.com
    http:
      paths:
      - backend:
          service:
            name: cortex-gw
            port:
              number: 80
        path: /
        pathType: Prefix
