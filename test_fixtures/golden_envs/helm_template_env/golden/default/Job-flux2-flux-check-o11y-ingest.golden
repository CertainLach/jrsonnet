apiVersion: batch/v1
hello: world
kind: Job
metadata:
  annotations:
    kustomize.toolkit.fluxcd.io/reconcile: disabled
    kustomize.toolkit.fluxcd.io/reconcile-disabled-by: platform-federal
    kustomize.toolkit.fluxcd.io/reconcile-disabled-reason: Ignored with the withFluxIgnore
      jsonnet utility. Flux not running in federal clusters
  name: flux2-flux-check-o11y-ingest
  namespace: default
spec:
  jobTemplate:
    spec:
      template:
        spec:
          containers:
          - args:
            - --reason=Removing Flux ignores before scheduled rollout of this cell
            - --namespace=metrics-ops-03
            - --selector=app.kubernetes.io/name=mimir
            image: bitnami/kubectl:1.25
            name: kubectl
  schedule: 0 9 * * 1-5
